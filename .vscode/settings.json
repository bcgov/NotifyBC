{
  "editor.rulers": [80],
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.trimAutoWhitespace": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": false,
    "source.fixAll.eslint": true
  },

  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,

  "typescript.tsdk": "./node_modules/typescript/lib",
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": false,
  "typescript.preferences.quoteStyle": "single",
  "eslint.run": "onSave",
  "eslint.nodePath": "./node_modules",
  "eslint.validate": ["javascript", "typescript"],
  "cSpell.words": [
    "Booter",
    "Defang",
    "ECONNECTION",
    "IDAQAB",
    "NDRs",
    "Ntfctn",
    "PVCs",
    "Vuex",
    "addmeta",
    "addt",
    "apitoken",
    "automigrate",
    "codecopy",
    "copyfiles",
    "createsend",
    "dovyp",
    "feedparser",
    "getopt",
    "gsmtp",
    "hsts",
    "ioredis",
    "jmespath",
    "jmespath's",
    "jwks",
    "lcov",
    "letsencrypt",
    "mailparser",
    "materialdesignicons",
    "mymaster",
    "nonexpired",
    "preauthorize",
    "quickstart",
    "randexp",
    "remoting",
    "resizer",
    "roboto",
    "serviceaccount",
    "signup",
    "smgov",
    "summernote",
    "testlab",
    "tolerations",
    "undelete",
    "universalid",
    "unsub",
    "unsubscript",
    "unsubscriptions",
    "usercredential",
    "userdisplayname",
    "yourprojectname"
  ]
}
